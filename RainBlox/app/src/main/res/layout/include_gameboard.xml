<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/md_blue_200">

    <ImageButton
        android:layout_width="@dimen/board_button_width"
        android:layout_height="@dimen/board_button_height"
        app:srcCompat="@drawable/vector_lock_white"
        android:id="@+id/imgbtntopcenter"
        app:layout_constraintBottom_toTopOf="@+id/imgbtncenter"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:layout_width="@dimen/board_button_width"
        android:layout_height="@dimen/board_button_height"
        app:srcCompat="@drawable/vector_lock_white"
        android:id="@+id/imageButton3"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/imgbtncenter"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp" />

    <ImageButton
        android:layout_width="@dimen/board_button_width"
        android:layout_height="@dimen/board_button_height"
        app:srcCompat="@drawable/vector_lock_white"
        android:id="@+id/imgbtncenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:layout_width="@dimen/board_button_width"
        android:layout_height="@dimen/board_button_height"
        app:srcCompat="@drawable/vector_lock_white"
        android:id="@+id/imgbtncenterright"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toRightOf="@+id/imgbtntopcenter"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageButton
        android:layout_width="@dimen/board_button_width"
        android:layout_height="@dimen/board_button_height"
        app:srcCompat="@drawable/vector_lock_white"
        android:id="@+id/imgbtntopright"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/imgbtncenterright"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toRightOf="@+id/imgbtntopcenter"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:layout_height="@dimen/board_button_height"
        app:srcCompat="@drawable/vector_lock_white"
        android:id="@+id/imgbtntopleft"
        app:layout_constraintRight_toLeftOf="@+id/imgbtntopcenter"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toTopOf="@+id/imageButton7"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.43"
        android:layout_width="@dimen/board_button_width" />

    <ImageButton
        android:layout_width="@dimen/board_button_width"
        android:layout_height="@dimen/board_button_height"
        app:srcCompat="@drawable/vector_lock_white"
        android:id="@+id/imgbtnbottomright"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/imgbtncenterright"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toRightOf="@+id/imageButton3"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp" />

    <ImageButton
        android:layout_width="@dimen/board_button_width"
        android:layout_height="@dimen/board_button_height"
        app:srcCompat="@drawable/vector_lock_white"
        android:id="@+id/imageButton10"
        app:layout_constraintRight_toLeftOf="@+id/imageButton3"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/imageButton7"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:layout_width="@dimen/board_button_width"
        android:layout_height="@dimen/board_button_height"
        app:srcCompat="@drawable/vector_lock_white"
        android:id="@+id/imageButton7"
        app:layout_constraintRight_toLeftOf="@+id/imgbtntopcenter"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:layout_width="@dimen/board_button_width"
        android:layout_height="@dimen/board_button_height"
        app:srcCompat="@drawable/vector_lock_white"
        android:id="@+id/imageButton5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>